<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="it.csi.mddtools.servicegen.presentation.ServicegenModelWizardID"
            name="%_UI_ServicegenModelWizard_label"
            class="it.csi.mddtools.servicegen.presentation.ServicegenModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/ServicegenModelFile.gif">
         <description>%_UI_ServicegenModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="it.csi.mddtools.servicegen.presentation.ServicegenEditorID"
            name="%_UI_ServicegenEditor_label"
            icon="icons/full/obj16/ServicegenModelFile.gif"
            extensions="servicegen"
            class="it.csi.mddtools.servicegen.presentation.ServicegenEditor"
            contributorClass="it.csi.mddtools.servicegen.presentation.ServicegenActionBarContributor">
      </editor>
   </extension>

</plugin>
